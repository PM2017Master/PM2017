<h1>事務員一覧</h1>
<table>
    <thead>
        <tr>
            <th>メールアドレス</th>
        </tr>
    </thead>
    <tbody>
        <% @staffs.each do |s| %>
        <tr>
            <td><%= s.email %></td>
            <td><%= link_to 'Destroy', admin_operate_staff_path(s.id), method: :delete, data: {confirm: "Are you sure?"}%></td>
        </tr>
        <% end %>
    </tbody>
</table>
